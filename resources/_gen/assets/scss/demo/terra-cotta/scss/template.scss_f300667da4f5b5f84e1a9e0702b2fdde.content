/*/////////////////////////////////
//////////   VARIABLES
////////////////////////////////*/
:root {
  --body-background: #f2f2f2;
  --heading-color: #333333;
  --body-text-color: #646464;
  --primary-theme-color: #564DC0;
  --secondary-theme-color: #34BFA3;
  --cards-bg-color: #fff;
  --box-shadow-1: 20px 37px 102px rgba(0,0,0,0.09);
  --box-shadow-1-hover: 20px 37px 102px rgba(0,0,0,0.2);
  --primary-font: 'Source Sans Pro', sans-serif;
  --text-white-color: #fff;
  --font-light: 300;
  --font-regular: 400;
  --font-semi-bold: 600;
  --font-bold: 700; }

body.dark-theme {
  --body-background: #18191A;
  --heading-color: #fff;
  --body-text-color: rgba(255, 255, 255, 0.7);
  --primary-theme-color: #2A2C2F;
  --secondary-theme-color: #54D28F;
  --cards-bg-color: #2A2C2F;
  --box-shadow-1: none;
  --box-shadow-1-hover: none; }

/*/////////////////////////////////
//////////   BODY LEVEL
////////////////////////////////*/
body {
  font-family: var(--primary-font);
  line-height: 200%;
  color: var(--body-text-color);
  background-color: var(--body-background);
  font-weight: var(--font-light);
  transition: 300ms ease;
  font-size: 18px; }

p {
  line-height: 170%; }

/*/////////////////////////////////
//////////   COMMON
////////////////////////////////*/
#category-cards-home {
  margin: 80px 0; }

.section-title {
  font-size: 50px;
  color: var(--heading-color);
  font-weight: var(--font-bold);
  text-align: center;
  margin-bottom: 70px;
  transition: 300ms ease; }

.category-card {
  background-color: var(--cards-bg-color);
  box-shadow: var(--box-shadow-1);
  transition: 300ms ease;
  border-radius: 6px;
  padding: 50px 34px;
  margin-bottom: 30px; }
  .category-card:hover {
    box-shadow: var(--box-shadow-1-hover); }
  .category-card .icon-holder {
    text-align: center; }
    .category-card .icon-holder i {
      font-size: 107px;
      font-weight: bolder;
      color: var(--secondary-theme-color);
      padding: 40px 0; }
  .category-card .content-holder .category-title {
    font-size: 28px;
    color: var(--heading-color);
    font-weight: var(--font-bold);
    margin-bottom: 20px; }
  .category-card .content-holder .category-description {
    font-size: 18px;
    margin-bottom: 40px;
    color: var(--body-text-color); }

.rounded-primary-btn {
  background-color: var(--secondary-theme-color);
  border-radius: 50px;
  padding: 12px 50px;
  margin: 0 auto;
  font-size: 18px;
  color: #fff;
  text-decoration: none;
  display: block;
  text-align: center;
  width: 70%;
  transition: 300ms ease;
  transform: scale(1); }
  .rounded-primary-btn .reveal-icon {
    position: relative;
    display: block;
    transition: 300ms ease; }
    .rounded-primary-btn .reveal-icon:before {
      content: '\f454';
      font-family: 'Phosphor';
      position: absolute;
      top: 2px;
      right: 35px;
      transition: 300ms ease;
      opacity: 0; }
  .rounded-primary-btn:hover {
    background-color: var(--secondary-theme-color);
    color: #fff; }
    .rounded-primary-btn:hover .reveal-icon {
      transform: translateX(-10px); }
      .rounded-primary-btn:hover .reveal-icon:before {
        right: 10px;
        opacity: 1; }
  .rounded-primary-btn:focus {
    transform: scale(0.95); }

.dark-mode-toggle {
  background-color: transparent;
  border: none;
  color: var(--text-white-color);
  font-size: 40px;
  position: absolute;
  top: 20px;
  right: 20px;
  transition: 300ms ease; }
  .dark-mode-toggle:focus {
    outline: transparent; }

.dark-theme .ph-sun {
  display: none; }

.dark-theme .ph-moon {
  display: inline-block; }

.ph-sun {
  display: inline-block; }

.ph-moon {
  display: none; }

#scroll-to-top {
  position: fixed;
  right: 30px;
  bottom: 30px;
  background: var(--cards-bg-color);
  border: none;
  padding: 10px;
  font-size: 30px;
  box-shadow: var(--box-shadow-1);
  border-radius: 50px;
  transition: 300ms ease;
  color: var(--heading-color);
  line-height: 0; }
  #scroll-to-top:hover {
    box-shadow: var(--box-shadow-1-hover); }
  #scroll-to-top:focus {
    outline: none; }

.icon-set .icon-set-holder {
  display: flex;
  align-items: center;
  justify-content: center;
  flex-direction: column; }
  .icon-set .icon-set-holder [class^="ph-"] {
    font-size: 50px;
    margin-bottom: 3px; }
  .icon-set .icon-set-holder .icon-name {
    color: #d63384;
    background: #ffe3df;
    padding: 0 7px;
    border-radius: 5px;
    font-size: 12px;
    text-align: center;
    cursor: pointer; }

#snackbar {
  visibility: hidden;
  min-width: 250px;
  margin-left: -125px;
  background-color: #333;
  color: #fff;
  text-align: center;
  border-radius: 6px;
  padding: 10px 40px;
  position: fixed;
  z-index: 1;
  left: 50%;
  bottom: 30px;
  font-size: 17px; }

#snackbar.show {
  visibility: visible;
  -webkit-animation: fadein 0.5s, fadeout 0.5s 2.5s;
  animation: fadein 0.5s, fadeout 0.5s 2.5s; }

@-webkit-keyframes fadein {
  from {
    bottom: 0;
    opacity: 0; }
  to {
    bottom: 30px;
    opacity: 1; } }

@keyframes fadein {
  from {
    bottom: 0;
    opacity: 0; }
  to {
    bottom: 30px;
    opacity: 1; } }

@-webkit-keyframes fadeout {
  from {
    bottom: 30px;
    opacity: 1; }
  to {
    bottom: 0;
    opacity: 0; } }

@keyframes fadeout {
  from {
    bottom: 30px;
    opacity: 1; }
  to {
    bottom: 0;
    opacity: 0; } }

.back-home {
  position: fixed;
  top: 200px;
  right: -87px;
  background: #fff;
  border: none;
  font-size: 24px;
  height: 50px;
