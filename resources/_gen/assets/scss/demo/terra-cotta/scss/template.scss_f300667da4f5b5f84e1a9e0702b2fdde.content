.table-of-content {
  position: sticky;
  top: 20px;
  padding: 0 60px; }
  .table-of-content ul {
    list-style: none;
    padding: 0; }
    .table-of-content ul li {
      margin-bottom: 20px;
      position: relative;
      transition: 300ms ease;
      line-height: 140%; }
      .table-of-content ul li:before {
        content: '';
        display: block;
        height: 28px;
        width: 6px;
        background-color: #ccc;
        position: absolute;
        top: 0;
        left: -15px; }
      .table-of-content ul li.has-children:after {
        content: '\f1ad';
        font-family: 'Phosphor';
        position: absolute;
        top: 0;
        right: 0;
        font-size: 22px;
        transition: 300ms ease; }
      .table-of-content ul li.has-children.active:after, .table-of-content ul li.has-children.active-parent:after {
        transform: rotate(90deg); }
      .table-of-content ul li.active:before {
        background-color: var(--secondary-theme-color); }
      .table-of-content ul li.active:after {
        color: var(--secondary-theme-color); }
      .table-of-content ul li.active > a {
        color: var(--secondary-theme-color); }
      .table-of-content ul li.active ul, .table-of-content ul li.active-parent ul {
        max-height: 100vh;
        margin-top: 20px; }
